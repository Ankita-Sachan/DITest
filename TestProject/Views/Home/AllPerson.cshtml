@using Entities.Models;
@model IEnumerable<Person>
@{
    ViewBag.Title = "AllPerson";
}

<h2>AllPerson</h2>
<table cellpadding="0" cellspacing="0">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>Age</th>
        <th>Gender</th>
    </tr>
    @foreach (Person person in Model)
    {
        <tr>
            <td>@person.id</td>
            <td>@person.Name</td>
            <td>@person.Age</td>
            <td>@person.Gender</td>
        </tr>
    }
</table>

